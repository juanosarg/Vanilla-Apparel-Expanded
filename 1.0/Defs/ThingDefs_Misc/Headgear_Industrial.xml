<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- ##### Industrial ##### -->
    <!-- +++++ Scarf +++++ -->
    <ThingDef ParentName="HatMakeableBase">
		<defName>VAE_Headgear_Scarf</defName>
		<label>scarf</label>
		<description>A warm and long piece of fabric protecting the neck from exposure to cold.</description>
		<graphicData>
			<texPath>Things/Headgear/Scarf/Scarf</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
        <generateCommonality>0.05</generateCommonality>
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>1200</WorkToMake>
			<Mass>0.12</Mass>
            <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0.6</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>0.8</EquipDelay>
		</statBases>
		<thingCategories Inherit="False">
			<li>Headgear</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Teeth</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Headgear/Scarf/Scarf</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>IndustrialBasic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
            </defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<researchPrerequisite>VAE_CasualWear</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
    </ThingDef>

    <!-- +++++ Beret +++++ -->
    <ThingDef ParentName="HatMakeableBase">
		<defName>VAE_Headgear_Beret</defName>
		<label>beret</label>
		<description>A soft, flat crowned hat. Often worn by the military as a symbol of valor.</description>
		<graphicData>
			<texPath>Things/Headgear/Beret/Beret</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
        <generateCommonality>0.5</generateCommonality>
		<costStuffCount>20</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>1800</WorkToMake>
			<Mass>0.08</Mass>
            <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0.1</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0.5</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>0.8</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<ShootingAccuracyPawn>0.05</ShootingAccuracyPawn>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
			<li>Headgear</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Headgear/Beret/Beret</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>IndustrialBasic</li>
				<li>IndustrialMilitaryAdvanced</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
            </defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<researchPrerequisite>VAE_MilitaryClothing</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
    </ThingDef>

    <!-- +++++ Hardhat +++++ -->
    <ThingDef ParentName="HatMakeableBase">
		<defName>VAE_Headgear_Hardhat</defName>
		<label>Hardhat</label>
		<description>A type of helmet predominantly used in a work environment. Provides high protection against blunt trauma.</description>
		<graphicData>
			<texPath>Things/Headgear/HardHat/HardHat</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
        <generateCommonality>0.15</generateCommonality>
		<costList>
			<Steel>45</Steel>
		</costList>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>3500</WorkToMake>
			<Mass>2</Mass>
			<ArmorRating_Sharp>0.12</ArmorRating_Sharp>
			<ArmorRating_Blunt>1</ArmorRating_Blunt>
			<ArmorRating_Heat>0.6</ArmorRating_Heat>
			<Insulation_Cold>0</Insulation_Cold>
 			<Insulation_Heat>4</Insulation_Heat>
            <EquipDelay>0.8</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<ConstructionSpeed>0.10</ConstructionSpeed>
			<ConstructSuccessChance>0.05</ConstructSuccessChance>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
			<li>Headgear</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Headgear/HardHat/HardHat</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>IndustrialBasic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
            </defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<researchPrerequisite>VAE_WorkAttire</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
		</recipeMaker>
    </ThingDef>

