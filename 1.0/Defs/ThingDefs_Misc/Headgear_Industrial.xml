    <!-- +++++ Gas Mask +++++ -->
    <ThingDef ParentName="HatMakeableBase">
		<defName>VAE_Headgear_GasMask</defName>
		<label>Gas Mask</label>
		<description>A full face isolating mask with basic air filtration systems, providing some degree of toxic immunity.</description>
		<graphicData>
			<texPath>Things/Headgear/GasMask/GasMask</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<tradeability>Sellable</tradeability>
        <generateCommonality>0.05</generateCommonality>
		<costList>
			<Steel>35</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>4800</WorkToMake>
			<Mass>1.05</Mass>
			<ArmorRating_Sharp>0.12</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.2</ArmorRating_Blunt>
			<ArmorRating_Heat>0.68</ArmorRating_Heat>
			<Insulation_Cold>4</Insulation_Cold>
 			<Insulation_Heat>2</Insulation_Heat>
            <EquipDelay>2.5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<ToxicSensitivity>-1</ToxicSensitivity>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
			<li>Headgear</li>
